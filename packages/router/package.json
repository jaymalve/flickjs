  },
  "homepage": "https://github.com/jaymalave/flick#readme",
  "bugs": {
    "url": "https://github.com/jaymalave/flick/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@flickjs/runtime": "link:@flickjs/runtime"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
